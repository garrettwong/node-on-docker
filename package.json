{
  "name": "docker_web_app",
  "version": "1.0.0",
  "description": "Node.js on Docker",
  "author": "garrettdotnet",
  "main": "server.js",
  "scripts": {
    "build": "babel server -s -D -d dist",
    "start": "nodemon -w src --exec \"babel-node server\"",
    "test": "eslint server/",
    "server_bigtable_tests": "nodemon -w src --exec \"babel-node server_bigtable_tests --presets es2015,stage-0\""
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "dependencies": {
    "@google-cloud/bigtable": "^0.15.0",
    "@google-cloud/storage": "^1.7.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "express": "^4.16.1",
    "morgan": "^1.9.1",
    "pug": "^2.0.3",
    "resource-router-middleware": "^0.7.0"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/npm/npm.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "acorn": "^7.1.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^5.3.0",
    "nodemon": "^2.0.2"
  }
}
